<div class="d-flex justify-content-between flex-wrap flex-md noweap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Adicionar Usuário</h1>
    <div class="btn-tollbar mb-2 mb-md 0">
        <div class="btn-group me-2">
            <a href="/paineldecontrole" class="btn btn-sm btn-outline-secondary">Painel de Controle</a>
        </div>
    </div>
</div>
<div class="col py-3">
    <div class="col text-end fw-lighter">
        <b>UserId</b>
    </div>
</div>
<form action="/addAdmin" method="POST">
    <div class="row form-group mb-4">
        <div class="col">
            <label for="nome">Nome</label>
            <input type="text" class="form-control" id="nome" name="nome" value="" placeholder="Nome" required>
        </div>
        <div class="col">
            <label for="email">E-mail</label>
            <input type="text" class="form-control" id="email" name="email" value="" placeholder="email@email.com" required>
        </div>
        <div class="col">
            <label for="senha">Senha</label>
            <input type="password" class="form-control" id="senha" name="senha" value="" placeholder="********" required>
        </div>
        <div class="col">
            <label for="ddd">DDD</label>
            <input type="number" class="form-control" id="ddd" name="ddd" value="" placeholder="00">
        </div>
        <div class="col">
            <label for="numero">Número</label>
            <input type="number" class="form-control" id="numero" name="numero" value="" placeholder="123456789">
        </div>
    </div>
    <div class="form-group mb-4">
        <button type="submit" class="btn btn-primary">Adicionar Usuário</button>
    </div>
</form>